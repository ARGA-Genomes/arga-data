{
    "dataType": "source",
    "dbType": "specific",
    "files": [
        {
            "url": "https://ftp.ebi.ac.uk/pub/databases/ena/genome_collections/gc.xml",
            "downloadedFile": "genomeCollections.xml",
            "processing": [
                {
                    "path": "tools/processing/xmlProcess.py",
                    "function": "process",
                    "args": [
                        "{INPUT, 0}",
                        "{PATH, PROCESSING, genomeCollections.csv}"
                    ],
                    "kwargs": {
                        "subfileRows": 150000,
                        "compressChild": [
                            "CHROMOSOMES",
                            "ASSEMBLY_LINKS",
                            "ASSEMBLY_ATTRIBUTES",
                            "ASSEMBLY"
                        ]
                    },
                    "outputs": [
                        "{PATH, PROCESSING, genomeCollections.csv}"
                    ]
                }
            ]
        }
    ]
}