{
    "dataType": "source",
    "dbType": "location",
    "dataLocation": "https://ftp.ebi.ac.uk/pub/databases/eva/rs_releases/release_4/by_species/",
    "subDirectoryDepth": -1,
    "regexMatch": ".*\\.txt\\.gz",
    "processing": [
        {
            "script": "tools/processing/extract.py",
            "function": "process",
            "args": [
                "{INPUT, 0, FILEPATH}"
            ],
            "outputs": [
                "{INPUTPATH, PROCESSING, 0, FILEPATH_STEM}"
            ]
        }
    ],
    "fileProperties": {
        "separator": "\t"
    }
}
