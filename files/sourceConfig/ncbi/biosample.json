{
    "dataType": "enrich",
    "dbType": "specific",
    "files": [
        {
            "url": "https://ftp.ncbi.nlm.nih.gov/biosample/biosample_set.xml.gz",
            "downloadedFile": "biosample_set.xml.gz",
            "processing": [
                {
                    "path": "tools/processing/extract.py",
                    "function": "process",
                    "args": [
                        "{INPUT, 0}"
                    ],
                    "outputs": [
                        "{PATH, PROCESSING, biosample_set.xml}"
                    ]
                },
                {
                    "path": "tools/processing/xmlProcess.py",
                    "function": "process",
                    "args": [
                        "{INPUT, 0}"
                    ],
                    "kwargs": {
                        "subfileRows": 1000000,
                        "onefile": "biosample",
                        "onlyTags": {
                            "BioSample": {"accession": []},
                            "Organism": {"taxonomy_id": []},
                            "Contact": {"email": [], "lab": []},
                            "Link": {"type": ["url"]},
                            "Attribute": {
                                "attribute_name": [
                                    "collection date",
                                    "speciment_voucher",
                                    "sex",
                                    "development stage",
                                    "tissue",
                                    "isolate",
                                    "lat_long",
                                    "geo_loc_name"
                                ]
                            }
                        }
                    },
                    "outputs": [
                        "{PATH, PROCESSING, biosample.csv}"
                    ]
                }
            ]
        }
    ],
    "dwcProperties": {
        "augment": [
            {
                "path": "sourceProcessing/ncbi/bioSamplePostDWC.py",
                "function": "augment"
            }
        ]
    }
}
