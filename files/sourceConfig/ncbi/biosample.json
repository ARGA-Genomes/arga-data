{
    "dataType": "enrich",
    "dbType": "specific",
    "files": [
        {
            "url": "https://ftp.ncbi.nlm.nih.gov/biosample/biosample_set.xml.gz",
            "downloadedFile": "biosample_set.xml.gz",
            "processing": [
                {
                    "path": "tools/processing/extract.py",
                    "function": "process",
                    "args": [
                        "{INPUT, 0}",
                        "{PATH, PROCESSING}"
                    ],
                    "outputs": [
                        "{PATH, PROCESSING, biosample_set.xml}"
                    ]
                },
                {
                    "path": "tools/processing/xmlProcess.py",
                    "function": "process",
                    "args": [
                        "{INPUT, 0}",
                        "{PATH, PROCESSING, biosample.csv}"
                    ],
                    "kwargs": {
                        "subfileRows": 150000,
                        "compressChild": [
                            "Paragraph",
                            "Attribute",
                            "Model",
                            "Id",
                            "Link",
                            "Synonym"
                        ],
                        "collectionExtract": {
                            "Attribute": {
                                "attribute_name": {
                                    "collection date": "collection date",
                                    "specimen_voucher": "specimen voucher",
                                    "sex": "sex",
                                    "developmental stage": "developmental stage",
                                    "tissue": "tissue",
                                    "isolate": "isolate",
                                    "lat_lon": "lat long",
                                    "geo_loc_name": "geo loc name"
                                }
                            }
                        }
                    },
                    "outputs": [
                        "{PATH, PROCESSING, biosample.csv}"
                    ]
                }
            ]
        }
    ],
    "dwcProperties": {
        "augment": [
            {
                "path": "sourceProcessing/ncbi/postDWC.py",
                "function": "biosample"
            }
        ]
    }
}
