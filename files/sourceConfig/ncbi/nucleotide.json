{
    "dataType": "source",
    "dbType": "location",
    "dataLocation": "https://ftp.ncbi.nlm.nih.gov/genbank/",
    "subDirectoryDepth": 0,
    "regexMatch": "gb[a-z]{3}\\d+\\.seq\\.gz",
    "processing": [
        {
            "script": "tools/processing/extract.py",
            "function": "process",
            "args": [
                "{INPUT, 0}"
            ],
            "outputs": [
                "{INPUT, 0, STEM}"
            ]
        },
        {
            "script": "sourceProcessing/ncbi/flatfileParser.py",
            "function": "process",
            "args": [
                "{INPUT, 0}"
            ],
            "outputs": [
                "{INPUT, 0, STEM, .csv}"
            ]
        }
    ]
}