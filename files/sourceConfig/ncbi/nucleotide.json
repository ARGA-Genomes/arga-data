{
    "dataType": "source",
    "dbType": "location",
    "dataLocation": "https://ftp.ncbi.nlm.nih.gov/genbank/",
    "subDirectoryDepth": 0,
    "regexMatch": "gb[a-z]{3}\\d+\\.seq\\.gz",
    "globalProcessing": [
        {
            "path": "tools/processing/extract.py",
            "function": "process",
            "args": [
                "{INPUT, 0, FILEPATH}",
                "{PATH, PROCESSING}"
            ],
            "outputs": [
                "{INPUTPATH, PROCESSING, 0, STEM}"
            ]
        },
        {
            "path": "sourceProcessing/ncbi/flatfileParser.py",
            "function": "process",
            "args": [
                "{INPUT, 0, FILEPATH}"
            ],
            "outputs": [
                "{INPUTPATH, PROCESSING, 0, FILEPATH_STEM, .csv}"
            ]
        }
    ]
}