{
    "dataType": "enrich",
    "dbType": "specific",
    "files": [
        {
            "url": "https://ftp.ncbi.nlm.nih.gov/bioproject/bioproject.xml",    
            "downloadedFile": "bioproject.xml",
            "processing": [
                {
                    "path": "tools/processing/xmlProcess.py",
                    "function": "process",
                    "args": [
                        "{INPUT, 0}",
                        "{PATH, PROCESSING, bioproject.csv}"
                    ],
                    "kwargs": {
                        "subfileRows": 150000,
                        "compressChild": [
                            "ProjectDescr",
                            "RepliconSet",
                            "ProjectLinks",
                            "AuthorSet",
                            "Grant",
                            "Description",
                            "Morphology",
                            "Objectives",
                            "ProjectDataTypeSet",
                            "BioSampleSet",
                            "IntendedDataTypeSet"
                        ]
                    },
                    "outputs": [
                        "{PATH, PROCESSING, bioproject.csv}"
                    ]
                }
            ]
        }
    ]
}
