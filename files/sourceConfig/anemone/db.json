{
    "dbType": "location",
    "dataLocation": "https://db.anemone.bio/dist/",
    "auth": "auth.txt",
    "folderPrefix": true,
    "regexMatch": ".*\\.tsv\\.xz",
    "globalProcessing": [
        {
            "path": "tools/processing/extract.py",
            "function": "process",
            "args": [
                "{INPUT, 0}",
                "{PATH, PROCESSING}"
            ],
            "outputs": [
                "{INPUTPATH, PROCESSING, 0, FILEPATH_STEM}"
            ]
        }
    ],
    "fileProperties": {
        "separator": "\t"
    },
    "dwcProperties": {
        "augment": [
            {
                "path": "sourceProcessing/anemone/postDWC.py",
                "function": "augment"
            }
        ]
    }
}
