{
    "dataType": "source",
    "dbType": "location",
    "dataLocation": "https://db.anemone.bio/dist/",
    "auth": "auth.txt",
    "regexMatch": ".*\\.tsv\\.xz",
    "processing": [
        {
            "script": "tools/processing/extract.py",
            "function": "process",
            "args": [
                "{INPUT, 0}"
            ],
            "outputs": [
                "{INPUT, 0, STEM}"
            ]
        }
    ],
    "fileProperties": {
        "separator": "\t"
    },
    "dwcProperties": {
        "augment": [
            {
                "script": "sourceProcessing/anemone/postDWC.py",
                "function": "augment"
            }
        ]
    }
}
