{
    "datasetID": "",
    "retrieveType": "url",
    "download": [
        {
            "url": "https://biodiversity.org.au/nsl/services/export/taxonCsv",
            "name": "apc.csv"
        }
    ],
    "processing": {
        "linear": [
            {
                "path": "./processing.py",
                "function": "denormalize",
                "args": [
                    "{IN-PATH}",
                    "{OUT-PATH}"
                ],
                "output": "demormalized.csv"
            },
            {
                "path": "./processing.py",
                "function": "cleanup",
                "args": [
                    "{IN-PATH}",
                    "{OUT-PATH}"
                ],
                "output": "all-taxa.csv"
            }
        ]
    },
    "update": {
        "type": "weekly",
        "day": "sunday",
        "time": 9,
        "repeat": 2
    }
}
