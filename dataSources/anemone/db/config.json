{
    "downloading": {
        "retrieveType": "crawl",
        "tasks": [
            {
                "url": "https://db.anemone.bio/dist/",
                "regex": ".*\\.tsv\\.xz",
                "urlPrefix": 2
            }
        ]
    },
    "processing": [
        {
            "parallel": true,
            "path": ".lib/zipping.py",
            "function": "extract",
            "args": [
                "{IN-PATH}",
                "{OUT-DIR}"
            ],
            "outputs": [
                "{IN-PATH.}"
            ]
        },
        {
            "parallel": true,
            "path": "./processing.py",
            "function": "splitHaploType",
            "args": [
                "{IN-PATH}"
            ],
            "outputs": [
                "{IN-PATH}"
            ]
        }
    ],
    "conversion": {
        "datasetID": "ARGA:TL:0000174",
        "mapID": 286004534
    },
    "updating": {
        "type": "weekly",
        "day": "sunday",
        "interval": 2
    }
}