{
    "datasetID": "ARGA:TL:0000174",
    "retrieveType": "crawl",
    "auth": "auth.txt",
    "downloading": {
        "url": "https://db.anemone.bio/dist/",
        "regex": ".*\\.tsv\\.xz",
        "urlPrefix": 2
    },
    "processing": {
        "parallel": [
            {
                "path": ".lib/zipping.py",
                "function": "extract",
                "args": [
                    "{IN-PATH}",
                    "{OUT-DIR}"
                ],
                "output": "{IN-PATH.}",
                "properties": {
                    "separator": "\t"
                }
            },
            {
                "path": "./processing.py",
                "function": "splitHaploType",
                "args": [
                    "{IN-PATH}"
                ],
                "output": "{IN-PATH}"
            }
        ]
    },
    "conversion": {
        "mapID": 286004534
    },
    "update": {
        "type": "weekly",
        "day": "sunday",
        "time": 9,
        "repeat": 2
    }
}
