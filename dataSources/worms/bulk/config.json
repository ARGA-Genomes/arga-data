{
    "downloading": {
        "retrieveType": "script",
        "tasks": [
            {
                "path": "./processing.py",
                "function": "retrieve",
                "args": [
                    "{OUT-PATH}"
                ],
                "outputs": [
                    "worms.zip"
                ]
            }
        ]
    },
    "processing": [
        {
            "path": ".lib/zipping.py",
            "function": "extract",
            "args": [
                "{IN-PATH}",
                "{OUT-DIR}"
            ],
            "outputs": [
                "{IN-PATH.}"
            ]
        },
        {
            "path": "./processing.py",
            "function": "combine",
            "args": [
                "{IN-PATH}",
                "{OUT-PATH}"
            ],
            "outputs": [
                "worms.csv"
            ]
        }
    ],
    "updating": {
        "type": "weekly",
        "day": "sunday",
        "interval": 2
    }
}