    "retrieveType": "crawl",
    "downloading": {
        "url": "https://ftp.ebi.ac.uk/pub/databases/ena/sequence/con-std_latest/",
        "regex": "[A-Z]{3}_<SUB:NAME>_\\d+\\.dat\\.gz",
        "maxDepth": 1
    },
    "processing": {
        "parallel": [
            {
                "path": "./parser.py",
                "function": "parse",
                "args": [
                    "{IN-PATH}",
                    "{OUT-PATH}"
                ],
                "output": "{IN-PATH..parquet}"
            }
        ],
        "linear": [
            {
                "path": ".lib/bigFiles.py",
                "function": "combineDirectoryFiles",
                "args": [
                    "{OUT-PATH}",
                    "{IN-DIR}",
                    "*.parquet",
                    true
                ],
                "kwargs": {
                    "float_format": "%.0f"
                },
                "output": "<SUB>.csv"
            }
        ]
    },
    "conversion": {
        "mapID": 1182330400,
        "chunkSize": 512
    },
    "update": {
        "type": "weekly",
        "day": "sunday",
        "time": 9,
        "repeat": 2
    }
}
