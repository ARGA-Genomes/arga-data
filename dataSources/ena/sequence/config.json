{
    "datasetID": "",
    "subsections": {
        "expressed": {
            "tags": {
                "name" : "EST"
            }
        },
        "genomeSurvey": {
            "tags": {
                "name": "GSS"
            }
        },
        "sequenceTagged": {
            "tags": {
                "name": "STS"
            }
        },
        "constructed": {
            "tags": {
                "name": "CON"
            }
        },
        "transcriptome": {
            "tags": {
                "name": "TSA"
            }
        },
        "assembledTranscriptome": {
            "tags": {
                "name": "HTC"
            }
        },
        "assembledSequence": {
            "tags": {
                "name": "HTG"
            }
        },
        "patent": {
            "tags": {
                "name": "PAT"
            }
        },
        "wholeGenome": {
            "tags": {
                "name": "WGS"
            }
        }
    },
    "retrieveType": "crawl",
    "downloading": {
        "url": "https://ftp.ebi.ac.uk/pub/databases/ena/sequence/con-std_latest/",
        "regex": "<SUB:NAME>_FUN_1\\.dat\\.gz",
        "maxDepth": 1
    },
    "processing": {
        "parallel": [
            {
                "path": "./processing.py",
                "function": "parse",
                "args": [
                    "{IN-PATH}",
                    "{OUT-PATH}"
                ],
                "output": "{IN-PATH.parquet}"
            }
        ],
        "linear": [
            {
                "path": ".lib/bigFiles.py",
                "function": "combineDirectoryFiles",
                "args": [
                    "{OUT-PATH}",
                    "{IN-DIR}",
                    "*.parquet"
                ],
                "output": "<SUB>.csv"
            }
        ]
    },
    "conversion": {
        "mapID": 1182330400,
        "chunkSize": 512
    },
    "update": {
        "type": "weekly",
        "day": "sunday",
        "time": 9,
        "repeat": 2
    }
}
