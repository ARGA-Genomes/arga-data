{
    "downloading": {
        "retrieveType": "url",
        "tasks": [
            {
                "url": "https://ftp.ebi.ac.uk/pub/databases/ena/taxonomy/taxonomy.xml.gz",
                "name": "taxonomy.xml.gz"
            }
        ]
    },
    "processing": {
        "linear": [
            {
                "path": ".lib/zipping.py",
                "function": "extract",
                "args": [
                    "{IN-PATH}",
                    "{OUT-DIR}"
                ],
                "output": "taxonomy.xml"
            },
            {
                "path": ".lib/xml.py",
                "function": "basicXMLProcessor",
                "args": [
                    "{IN-PATH}",
                    "{OUT-PATH}",
                    100000
                ],
                "output": "taxonomy.csv"
            }
        ]
    },
    "conversion": {
        "datasetID": "ARGA:TL:0002004",
        "mapID": 1058330275
    },
    "updating": {
        "type": "weekly",
        "day": "sunday",
        "interval": 2
    }
}