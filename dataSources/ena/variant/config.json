    "downloading": {
        "url": "https://ftp.ebi.ac.uk/pub/databases/eva/rs_releases/release_4/by_species/",
        "regex": ".*\\.txt\\.gz"
    },
    "processing": {
        "parallel": [
            {
                "path": ".../zipping.py",
                "function": "extract",
                "args": [
                    "{IN-PATH}",
                    "{OUT-DIR}"
                ],
                "output": "{IN-PATH.}",
                "properties": {
                    "sep": "\t"
                }
            }
        ]
    },
    "conversion": {
        "mapID": 1058330275
    },
    "update": {
        "type": "weekly",
        "day": "sunday",
        "time": 9,
        "repeat": 2
    }
}
