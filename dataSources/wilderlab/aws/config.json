    "downloading": {
        "retrieveType": "url",
        "tasks": [
            {
                "url": "https://s3.ap-southeast-2.amazonaws.com/wilderlab.publicdata/<SUB>3.csv",
                "name": "<SUB>.csv"
            }
        ]
    },
    "processing": [
        {
