{
    "datasetID": "",
    "retrieveType": "crawl",
    "downloading": {
        "location": "https://www.arb-silva.de/no_cache/download/archive/current/Exports/full_metadata/",
        "link": "https://www.arb-silva.de/",
        "regex": ".*\\.gz$",
        "maxDepth": 0
    },
    "processing": {
        "parallel": [
            {
                "path": "lib/tools/zipping.py",
                "function": "extract",
                "args": [
                    "{IN-PATH}",
                    "{OUTDIR}"
                ],
                "output": "{IN-STEM}",
                "properties": {
                    "separator": "\t"
                }
            }
        ]
    }
}
