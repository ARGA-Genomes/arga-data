{
    "downloading": {
        "retrieveType": "script",
        "tasks": [
            {
                "path": "./processing.py",
                "function": "collectBiocache",
                "args": [
                    {
                        "q": "*:*",
                        "fq": "(basis_of_record:\"PRESERVED_SPECIMEN\" OR basis_of_record:\"MATERIAL_SAMPLE\" OR basis_of_record:\"LIVING_SPECIMEN\" OR basis_of_record:\"MATERIAL_CITATION\")",
                        "disableAllQualityFilters": true,
                        "qualityProfile": "ALA",
                        "qc": "-_nest_parent_:*"
                    },
                    "{OUT-PATH}"
                ],
                "outputs": [
                    "biocache.zip"
                ]
            }
        ]
    },
    "processing": [
        {
            "path": ".lib/zipping.py",
            "function": "extract",
            "args": [
                "{IN-PATH}",
                "{OUT-DIR}"
            ],
            "outputs": [
                "biocache"
            ]
        },
        {
            "path": "./processing.py",
            "function": "cleanup",
            "args": [
                "{IN-PATH}",
                "{OUT-PATH}"
            ],
            "outputs": [
                "biocache.csv"
            ]
        }
    ],
    "conversion": {
        "datasetID": "0007001",
        "mapColumnName": "ala-biocache mappings"
    },
    "updating": {
        "type": "weekly",
        "day": "sunday",
        "interval": 2
    }
}