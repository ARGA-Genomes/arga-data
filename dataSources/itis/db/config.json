{
    "downloading": {
        "retrieveType": "url",
        "tasks": [
            {
                "url": "https://www.itis.gov/downloads/itisSqlite.zip",
                "name": "itisSqlite.zip"
            }
        ]
    },
    "processing": [
        {
            "path": ".lib/zipping.py",
            "function": "extract",
            "args": [
                "{IN-PATH}",
                "{OUT-DIR}"
            ],
            "outputs": [
                "{IN-PATH.}"
            ]
        },
        {
            "path": "./processing.py",
            "function": "unpack",
            "args": [
                "{IN-PATH}",
                "{OUT-PATH}"
            ],
            "outputs": [
                "dumps"
            ]
        },
        {
            "path": "./processing.py",
            "function": "combine",
            "args": [
                "{IN-PATH}",
                "{OUT-PATH}"
            ],
            "outputs": [
                "itis.csv"
            ]
        }
    ],
    "conversion": {
        "datasetID": "ARGA:TL:0001016"
    },
    "updating": {
        "type": "weekly",
        "day": "sunday",
        "interval": 2
    }
}