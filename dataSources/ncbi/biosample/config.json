{
    "downloading": {
        "retrieveType": "url",
        "tasks": [
            {
                "url": "https://ftp.ncbi.nlm.nih.gov/biosample/biosample_set.xml.gz",
                "name": "biosample_set.xml.gz"
            }
        ]
    },
    "processing": [
        {
            "path": ".lib/zipping.py",
            "function": "extract",
            "args": [
                "{IN-PATH}",
                "{OUT-DIR}"
            ],
            "outputs": [
                "biosample_set.xml"
            ]
        },
        {
            "path": ".lib/xml.py",
            "function": "basicXMLProcessor",
            "args": [
                "{IN-PATH}",
                "{OUT-PATH}",
                150000
            ],
            "outputs": [
                "rawBiosample.csv"
            ]
        },
        {
            "path": "./processing.py",
            "function": "splitLatLong",
            "args": [
                "{IN-PATH}",
                "{OUT-PATH}"
            ],
            "outputs": [
                "biosample.csv"
            ]
        }
    ],
    "conversion": {
        "datasetID": "ARGA:TL:0002001",
        "mapID": 109194600
    },
    "updating": {
        "type": "weekly",
        "day": "sunday",
        "interval": 2
    }
