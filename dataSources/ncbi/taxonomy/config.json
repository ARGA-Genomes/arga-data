{
    "datasetID": "ARGA:TL:0002000",
    "retrieveType": "url",
    "downloading": [
        {
            "url": "https://ftp.ncbi.nlm.nih.gov/pub/taxonomy/taxdmp.zip",
            "name": "taxdump.zip"
        }
    ],
    "processing": {
        "linear": [
            {
                "path": ".lib/zipping.py",
                "function": "extract",
                "args": [
                    "{IN-PATH}",
                    "{OUT-DIR}"
                ],
                "output": "taxdump"
            },
            {
                "path": "./processing.py",
                "function": "parse",
                "args": [
                    "{IN-PATH}",
                    "{OUT-PATH}"
                ],
                "output": "ncbiTaxonomy.csv"
            }
        ]
    },
    "conversion": {
        "mapID": 240630744,
        "chunkSize": 65536
    },
    "update": {
        "type": "weekly",
        "day": "sunday",
        "time": 9,
        "repeat": 2
    }
}
