{
    "retrieveType": "url",
    "downloading": [
        {
            "url": "https://ftp.ncbi.nlm.nih.gov/bioproject/bioproject.xml",    
            "name": "bioproject.xml"
            
        }
    ],
    "processing": {
        "linear": [
            {
                "path": ".lib/xml.py",
                "function": "basicXMLProcessor",
                "args": [
                    "{IN-PATH}",
                    "{OUT-PATH}",
                    150000
                ],
                "output": "bioproject.csv"
            }
        ]
    },
    "update": {
        "type": "weekly",
        "day": "sunday",
        "time": 9,
        "repeat": 2
    }
}
