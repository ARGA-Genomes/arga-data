{
    "retrieveType": "url",
    "download": {
        "files": [
            {
                "url": "https://ftp.ncbi.nlm.nih.gov/bioproject/bioproject.xml",    
                "name": "bioproject.xml"
                
            }
        ]
    },
    "processing": {
        "final": [
            {
                "path": "tools/processing/xmlProcess.py",
                "function": "process",
                "args": [
                    "{INPATH}",
                    "{OUTPATH}"
                ],
                "kwargs": {
                    "subfileRows": 150000,
                    "compressChild": [
                        "ProjectDescr",
                        "RepliconSet",
                        "ProjectLinks",
                        "AuthorSet",
                        "Grant",
                        "Description",
                        "Morphology",
                        "Objectives",
                        "ProjectDataTypeSet",
                        "BioSampleSet",
                        "IntendedDataTypeSet"
                    ]
                },
                "output": "bioproject.csv"
            }
        ]
    },
    "update": {
        "type": "weekly",
        "day": "sunday",
        "time": 9,
        "repeat": 2
    }
}
