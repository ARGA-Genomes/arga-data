{
    "dbType": "location",
    "dataLocation": "https://ftp.ncbi.nlm.nih.gov/genbank/",
    "subDirectoryDepth": 0,
    "regexMatch": "gbmam\\d+\\.seq\\.gz",
    "finalProcessing": [
        {
            "path": "sourceProcessing/ncbi.py",
            "function": "compileNucleotide",
            "args": [
                "{PATH, DOWNLOAD}",
                "{PATH, PROCESSING, nucMammalian.csv}"
            ],
            "outputs": [
                "{PATH, PROCESSING, nucMammalian.csv}"
            ]
        }
    ],
    "dwcProperties": {
        "skipRemap": [
            "accession",
            "version",
            "fasta_url"
        ]
    }
}