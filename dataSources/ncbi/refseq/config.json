{
    "datasetID": "ARGA:TL:0002002",
    "retrieveType": "url",
    "download": {
        "files": [
            {
                "url": "https://ftp.ncbi.nih.gov/genomes/refseq/assembly_summary_refseq.txt",
                "name": "assembly_summary_refseq.tsv",
                "properties": {
                    "separator": "\t",
                    "firstrow": 1
                }
            }
        ]
    },
    "processing": {
        "final": [
            {
                "path": "../sharedProcessing.py",
                "function": "enrichStats",
                "args": [
                    "{INFILE}",
                    "{OUTPATH}",
                    "./apikey.txt"
                ],
                "output": "refseq.csv"
            }
        ]
    },
    "conversion": {
        "mapID": 2003682060,
        "customMapPath": "./customColumns.json",
        "setNA": [
            "na"
        ],
        "fillNA": {
            "assemblies": {
                "datasetID": {
                    "assemblies": [
                        "sequence_id"
                    ],
                    "annotations": [
                        "sequence_id"
                    ],
                    "depositions": [
                        "sequence_id"
                    ],
                    "sequences": [
                        "record_id"
                    ]
                }
            }
        }
    },
    "update": {
        "type": "weekly",
        "day": "sunday",
        "time": 9,
        "repeat": 1
    }
}
