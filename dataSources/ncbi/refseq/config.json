{
    "downloading": {
        "retrieveType": "url",
        "tasks": [
            {
                "url": "https://ftp.ncbi.nih.gov/genomes/refseq/assembly_summary_refseq.txt",
                "name": "assembly_summary_refseq.tsv",
                "properties": {
                    "header": 1
                }
            }
        ]
    },
    "processing": [
        {
            "path": ".llib/sharedProcessing.py",
            "function": "getStats",
            "args": [
                "{IN-FILE}",
                "{OUT-PATH}"
            ],
            "outputs": [
                "apiData.csv"
            ]
        },
        {
            "path": ".llib/sharedProcessing.py",
            "function": "merge",
            "args": [
                "{D0-FILE}",
                "{IN-PATH}",
                "{OUT-PATH}"
            ],
            "outputs": [
                "refseq.csv"
            ]
        },
            {
                "path": ".llib/sharedProcessing.py",
                "function": "cleanData",
                "args": [
                    "{IN-PATH}",
                    "{OUT-PATH}"
                ],
                "outputs": [
                    "cleanRefseq.csv"
                ]
            }
    ],
    "conversion": {
        "datasetID": "ARGA:TL:0002002",
        "mapID": 2003682060
    },
    "updating": {
        "type": "weekly",
        "repeatDay": "sunday",
        "repeatFreq": 1
    }
}
