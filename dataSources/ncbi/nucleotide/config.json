        "url": "https://ftp.ncbi.nlm.nih.gov/genbank/",
        "regex": "gb{SUBSECTION:NAME}\\d+\\.seq\\.gz",
        "maxDepth": 0
    },
    "processing": {
        "parallel": [
            {
                "path": "./processing.py",
                "function": "parse",
