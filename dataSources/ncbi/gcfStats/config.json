{
    "retrieveType": "crawl",
    "download": {
        "url": "https://ftp.ncbi.nlm.nih.gov/genomes/all/GCF/",
        "regex": ".*assembly_stats\\.txt"
    },
    "processing": {
        "final": [
            {
                "path": "sourceProcessing/ncbi.py",
                "function": "compileAssemblyStats",
                "outputs": [
                    "gcfStats.csv"
                ],
                "additionalArgs": [
                    "{FOLDER, DOWNLOAD}"
                ]
            }
        ]
    }
}