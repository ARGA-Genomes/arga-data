{
    "datasetID": "ARGA:TL:0002003",
    "retrieveType": "url",
    "downloading": [
        {
            "url": "https://ftp.ncbi.nih.gov/genomes/genbank/assembly_summary_genbank.txt",
            "name": "assembly_summary_genbank.tsv",
            "properties": {
                "separator": "\t",
                "firstrow": 1
            }
        }
    ],
    "processing": {
        "linear": [
            {
                "path": "../sharedProcessing.py",
                "function": "getStats",
                "args": [
                    "{IN-FILE}",
                    "{OUT-PATH}",
                    "./apikey.txt"
                ],
                "output": "apiData.csv"
            },
            {
                "path": "../sharedProcessing.py",
                "function": "merge",
                "args": [
                    "{D0-FILE}",
                    "{IN-PATH}",
                    "{OUT-PATH}"
                ],
                "output": "genbank.csv"
            }
        ]
    },
    "conversion": {
        "mapID": 2003682060,
        "augment": [
            {
                "path": "../sharedProcessing.py",
                "function": "genbankAugment"
            }
        ]
    },
    "update": {
        "type": "weekly",
        "day": "sunday",
        "time": 9,
        "repeat": 1
    }
}
