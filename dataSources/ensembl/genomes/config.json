        },
        "fungi": {
            "id": "ARGA:TL:0002011",
            "name": "Fungi"
        }
    },
    "retrieveType": "url",
    "downloading": {
        "files": [
            {
                "url": "http://ftp.ensemblgenomes.org/pub/{SUBSECTION}/current/species_metadata_Ensembl{SUBSECTION:NAME}.json",
                "name": "{SUBSECTION}.json"
            },
            {
                "url": "http://ftp.ensemblgenomes.org/pub/{SUBSECTION}/current/species_Ensembl{SUBSECTION:NAME}.txt",
                "name": "{SUBSECTION}.tsv",
                "properties": {
                    "sep": "\t"
                }
            }
        ]
    },
    "processing": {
        "specific": {
            "0": [
                {
                    "path": "./processing.py",
                    "function": "flatten",
                    "args": [
                        "{IN-PATH}",
                        "{OUT-PATH}"
                    ],
                    "output": "metadata.csv"
                }
            ],
            "1": [
                {
                    "path": "./processing.py",
                    "function": "enrich",
                    "args": [
                        "{IN-PATH}",
                        "{SUBSECTION}",
                        "{OUT-PATH}"
                    ],
                    "output": "enrichedStats.csv"
                }
            ]
        },
        "linear": [
            {
                "path": "./processing.py",
                "function": "combine",
                "args": [
                    "{INPATH_0}",
                    "{INPATH_1}",
                    "{OUT-PATH}"
                ],
                "output": "{SUBSECTION}.csv"
            }
        ]
    }
}
