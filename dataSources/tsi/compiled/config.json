{
    "downloading": {
        "retrieveType": "script",
        "tasks": [
            {
                "path": "./processing.py",
                "function": "collectFromSheets",
                "args": [
                    "{OUT-DIR}"
                ],
                "outputs": [
                    "TSICompiled/collecting.csv",
                    "TSICompiled/lab_samples.csv",
                    "TSICompiled/organisms.csv",
                    "TSICompiled/tissues.csv"
                ]
            }
        ]
    },
    "compiling": {
        "files": [
            "{D0-PATH}",
            "{D1-PATH}",
            "{D2-PATH}",
            "{D3-PATH}",
            "../../meta.toml"
        ]
    },
    "updating": {
        "type": "weekly",
        "day": "sunday",
        "interval": 2
    }
}