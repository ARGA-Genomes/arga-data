{
    "NCBI-genbankSummary": {
        "uri": "https://ftp.ncbi.nih.gov/genomes/genbank/assembly_summary_genbank.txt",
        "downloadedFile": "assembly_summary_genbank.csv"
    },
    "NCBI-refseqSummary": {
        "uri": "https://ftp.ncbi.nih.gov/genomes/refseq/assembly_summary_refseq.txt",
        "downloadedFile": "assembly_summary_refseq.csv",
        "parseKwargs": {
            "header": 1,
            "sep": "\t"
        }
    },
    "NCBI-bioproject": {
        "uri": "https://ftp.ncbi.nlm.nih.gov/bioproject/bioproject.xml",    
        "downloadedFile": "bioproject.xml",
        "processing": [
            "xmlProcess.py"
        ],
        "processedFile": "bioproject.csv"
    },
    "NCBI-biosample": {
        "uri": "https://ftp.ncbi.nlm.nih.gov/biosample/biosample_set.xml.gz",
        "downloadedFile": "biosample_set.xml.gz",
        "processing": [
            "extract",
            "xmlSingleProcess.py"
        ],
        "processedFile": "biosample.csv"
    },
    "NCBI-biocollection": {
        "uri": [
            "https://ftp.ncbi.nih.gov/pub/taxonomy/biocollections/Collection_codes.txt",
            "https://ftp.ncbi.nih.gov/pub/taxonomy/biocollections/Institution_codes.txt",
            "https://ftp.ncbi.nih.gov/pub/taxonomy/biocollections/Unique_institution_codes.txt"
        ],
        "downloadedFile": [
            "Collection_codes.txt",
            "Institution_codes.txt",
            "Unique_institution_codes.txt"
        ],
        "processing": [
            "ncbiBiocollections.py"
        ],
        "processedFile": "biocollections.csv"
    },
    "NCBI-gene" : {
        "uri": "https://ftp.ncbi.nlm.nih.gov/gene/DATA/GENE_INFO/All_Data.gene_info.gz",
        "downloadedFile": "All_data.gene_info.gz",
        "processing": [
            "extract"
        ]
    },
    "NCBI-taxonomy": {
        "uri": "https://ftp.ncbi.nlm.nih.gov/pub/taxonomy/taxdmp.zip",
        "downloadedFile": "taxdump.zip",
        "processing": [
            "extract"
        ]
    },
    "NCBI-genbank": {
        "fileLoc": "https://ftp.ncbi.nlm.nih.gov/genbank/"
    },
    "NCBI-refseq": {
        "fileLoc": "https://ftp.ncbi.nlm.nih.gov/genomes/refseq/"
    },
    "NCBI-sra": {
        "fileLoc": "https://ftp.ncbi.nlm.nih.gov/sra/data/SRP125168/"
    },
    "NCBI-tpa": {
        "fileLoc": "https://ftp.ncbi.nlm.nih.gov/tpa/release/"
    },
    "ENA-assembly": {
        "uri": "https://www.ebi.ac.uk/ena/portal/api/search?result=assembly&fields=all&limit=0&format=tsv",
        "downloadedFile": "ena_assembly_all_fields.tsv",
        "parseKwargs": {
            "sep": "\t"
        }
    },
    "BOLD-australia": {
        "uri": "http://v4.boldsystems.org/index.php/API_Public/combined?geo=Australia&format=tsv",
        "downloadedFile": "bold_data.txt",
        "parseKwargs": {
            "sep": "\t"
        }
    }
}
